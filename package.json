{
  "name": "elastic-full-search",
  "description": "Functions to get full results from elasticsearch searches.",
  "version": "1.0.0",
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "dependencies": {
    "fast-json": "1.x.x",
    "utjs": "2.x.x"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "istanbul": "0.4.x",
    "jscs": "3.x.x",
    "jshint": "2.x.x",
    "mocha": "3.x.x",
    "sinon": "1.x.x"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "mocha test/*.js",
    "test:cover": "istanbul cover _mocha test/*.js",
    "jshint": "jshint index.js lib/*.js test/*.js example/*.js",
    "jscs": "jscs index.js lib/*.js test/*.js example/*.js",
    "jscs:fix": "jscs --fix index.js lib/*.js test/*.js example/*.js",
    "check": "npm run test && npm run jshint && npm run jscs && npm outdated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alemures/elastic-full-search.git"
  }
}
