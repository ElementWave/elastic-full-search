{
  "name": "elastic-full-search",
  "description": "Functions to get full results from elasticsearch searches.",
  "version": "0.0.1",
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "dependencies": {
    "utjs": "1.x.x"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "jscs": "3.x.x",
    "jshint": "2.x.x",
    "mocha": "3.x.x"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "mocha test/*.js",
    "jshint": "jshint index.js lib/*.js test/*.js",
    "jscs": "jscs index.js lib/*.js test/*.js",
    "jscs:fix": "jscs --fix index.js lib/*.js test/*.js",
    "check": "npm run test && npm run jshint && npm run jscs && npm outdated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alemures/elastic-full-search.git"
  }
}
